<If "%{REQUEST_URI} == '/' || %{REQUEST_URI} == '/pay'">
    <IfModule mod_shib>
      AuthType shibboleth
      require shibboleth
      ShibRequireSession on
    </IfModule>
</If>
<IfModule mod_rewrite.c>
    Options -MultiViews
    RewriteEngine On
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.*)$ index.php [QSA,L]
</IfModule>